def konvertalas(szoveg):
    szoveg=szoveg.upper()
    lekodolt=""
    for characters in szoveg:
        lekodolt=lekodolt+kod[characters]+"|"
    return lekodolt

def feltor(jelek):
    jelek=jelek.split(" ")
    feltorni=""
    for char in jelek:
        feltorni+=revkod[char]
    return feltorni

